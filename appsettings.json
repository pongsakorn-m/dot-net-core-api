{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MaxLoginFail": "3", // how many times allow to login fail
  "LockAccount": "1", // minutes for lock account to login when login fail > MaxLoginFail
  "JwtConfig": {
    "Secret": "ShoppingFoodSecret",
    "TokenExpire": "6", //  how long access token will expired
    "Issuer": "https://localhost:5000/",
    "Audience": "https://localhost:5000/"
  },
  "CorsAllows": "http://localhost:4200;http://localhost:4201" // cors allows
}
